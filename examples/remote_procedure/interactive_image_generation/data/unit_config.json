          {
            "instruction": "Click \"Start\" button to continue",
            "rows": [
              {
                "fields": [
                  {
                    "id": "id_prompt_0",
                    "label": "Please provide a detailed prompt to algorithm to generate an image that resemples the target image above",
                    "name": "prompt_0",
                    "type": "textarea",
                    "validators": {
                      "minLength": 20,
                      "maxLength": 500,
                      "required": true
                    }
                  }
                ]
              },
              {
                "classes": "custom--row-with-buttons",
