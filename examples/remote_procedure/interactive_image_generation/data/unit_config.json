          {
            "title": "Personal information",
            "instruction": "",
            "rows": [
              {
                "fields": [
                  {
                    "help": "",
                    "id": "id_age",
                    "label": "Age",
                    "name": "age",
                    "placeholder": "Type your age",
                    "tooltip": "Your age",
                    "type": "input",
                    "validators": {
                      "required": true,
                      "regexp": ["^[0-9]+$", "ig"]
                    },
                    "value": ""
                  },
                  {
                    "id": "id_gender",
                    "label": "Gender",
                    "multiple": false,
                    "name": "gender",
                    "options": [
                      {
                        "label": "---",
                        "value": ""
                      },
                      {
                        "label": "Male",
                        "value": "male"
                      },
                      {
                        "label": "Female",
                        "value": "female"
                      },
                      {
                        "label": "Other",
                        "value": "other"
                      }
                    ],
                    "type": "select",
                    "validators": {
                      "required": true
                    },
                    "value": ""
                  }
                ]
              }
            ]
          }
        ]
      },
      {
